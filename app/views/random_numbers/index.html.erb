
<p id="notice"><%= notice %></p>
<h1>Listing random_numbers</h1>

<table>
  <tr>
    <th>id</th>
    
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @random_numbers.each do |random_number| %>
  <tr>
  <td><%= random_number.id %></td>
    <td><%= random_number.Name %></td>
    <td><%= link_to 'Show', random_number %></td>
    <td><%= link_to 'Edit', edit_random_number_path(random_number) %></td>
    <td><%= link_to 'Destroy', random_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Random number', new_random_number_path %>
<%= form_tag import_random_numbers_path, multipart: true do %>
   <%= file_field_tag :file, accept: 'csv' %>
   <%= submit_tag "Import" %>
<% end %>